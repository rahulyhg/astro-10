{
  "private": true,
  "name": "Prototype",
  "description": "Super slick prototype",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --progress --colors --hide-modules --display-error-details --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "cross-env NODE_ENV=development webpack --watch --colors --hide-modules --display-error-details --config=node_modules/laravel-mix/setup/webpack.config.js",
    "hot": "cross-env NODE_ENV=development webpack-dev-server --hot --colors --content-base=public/ --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production webpack --progress --colors --hide-modules --display-error-details --config=node_modules/laravel-mix/setup/webpack.config.js",
    "refresh": "php artisan migrate:refresh && php artisan db:seed"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bulma": "^0.4.0",
    "chart.js": "^2.5.0",
    "cms-prototype-blocks": "git+ssh://git@github.com/unikent/cms-prototype-blocks.git#master",
    "element-ui": "^1.2.8",
    "jquery": "^3.1.0",
    "lodash": "^4.17.4",
    "normalize.css": "^5.0.0",
    "velocity-animate": "^1.4.2",
    "vue": "^2.1.0",
    "vue-chartjs": "^2.3.9",
    "vue-router": "^2.2.0",
    "vuedraggable": "^2.8.1-rc0",
    "vuex": "^2.1.1"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.8.1",
    "babel-eslint": "^7.2.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "cross-env": "^3.0.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-vue": "^2.0.1",
    "external-svg-sprite-loader": "^2.1.1",
    "laravel-mix": "^0.8.8",
    "svg-sprite-loader": "^0.3.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
